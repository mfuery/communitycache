  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --config webpack.dev.js",
    "upload": "node upload.js",
    "deploy:minor": "npm version minor && npm run build && npm run upload && eb deploy -l `git describe`",
    "deploy:patch": "npm version patch && npm run build && npm run upload && eb deploy -l `git describe`",
    "dev-server": "webpack-dev-server --config webpack.dev.js",
    "npminstall": "npm i -q",
    "develop": "npm run npminstall && npm run dev-server",
    "build": "webpack --config webpack.prod.js --progress --profile --color",
    "cleandist": "rm -rf /dist/*",
    "build-prod": "npm run npminstall && npm run cleandist && npm run build"
  },
